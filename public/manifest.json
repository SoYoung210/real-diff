{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Real Diff",
  "description": "Calculate Pull Request Real Diff",
  "browser_action": {
    "default_icon": "icon/icon_16x16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon/icon_16x16.png",
    "48": "icon/icon_64x64.png",
    "128": "icon/icon_128x128.png"
  },
  "permissions": ["tabs", "activeTab", "storage"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+K",
        "linux": "Alt+Shift+K",
        "chromeos": "Alt+Shift+K",
        "mac": "Command+Shift+K",
        "default": "Ctrl+Shift+K"
      },
      "description": "open_popup"
    }
  },
  "content_security_policy":
    "script-src 'self' 'sha256-VCSQgQD+u6aI1GbN9qMh4VqJYnazKlasWFYPQspyPJI='; object-src 'self'"
}
